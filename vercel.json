{
  "buildCommand": "remix build",
  "devCommand": "remix dev",
  "framework": "remix",
  "installCommand": "npm install",
  "functions": {
    "app/routes/**/*.jsx": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}